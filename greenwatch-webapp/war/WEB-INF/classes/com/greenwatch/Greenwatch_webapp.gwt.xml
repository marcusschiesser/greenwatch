<?xml version="1.0" encoding="UTF-8"?>
<module
	rename-to='greenwatch_webapp'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits
		name='com.google.gwt.user.User' />
	<!-- Inherits the Restlet core module -->
	<inherits
		name='org.restlet.Restlet' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits
		name='com.google.gwt.user.theme.standard.Standard' />

	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->

	<!-- Specify the app entry point class. -->
	<entry-point
		class='com.greenwatch.client.Greenwatch_webapp' />

	<!-- Specify the paths for translatable code -->
	<source
		path='client' />
	<source
		path='shared' />

	<!-- google maps -->
	<inherits
		name='com.google.gwt.maps.GoogleMaps' />

	<!-- Other module inherits -->
	<inherits
		name="com.google.code.gwt.geolocation.Html5Geolocation" />
</module>
